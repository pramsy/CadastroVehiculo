@model CadastroVehiculo.Models.Gearbox
@{
    ViewData["Title"] = "CadastrarCambio";
}
<h3>Adicionar Tipo de cambio</h3>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="GearboxType" class="control-label"></label>
                <input asp-for="GearboxType" class="form-control" />
                <span asp-validation-for="GearboxType" class="text-danger"></span>
            </div>
            <p>
                <div class="form-group">
                    <input type="submit" value="Cadastrar" class="btn btn-primary" />
                </div>
            </p>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
